//given an array of integers any pairs of 2 integers get added to the total number of pairs
return pairs

function SockMerchant(n, ar){
  const tracker = {};
  let pairs = 0;
  for (let i = 0; i < n; i++){
  
    if (tracker[ar[i]]) {
      pairs++;
      tracker[ar[i]] = 0;
     } else {
     tracker[ar[i]] = 1;
  }
  return pairs
}
